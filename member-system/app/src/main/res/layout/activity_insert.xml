<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertActivity">

    <TextView
            android:id="@+id/insertPageTitle"
            android:text="회원 추가하기"
            android:textSize="25sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.088"
            app:layout_constraintVertical_bias="0.051"/>
    <TextView
            android:id="@+id/nameSpaceTitle"
            android:text="성명"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.158"
            app:layout_constraintVertical_bias="0.255"/>
    <TextView
            android:id="@+id/ageSpaceTitle"
            android:text="나이"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.158"
            app:layout_constraintVertical_bias="0.375"/>
    <EditText
            android:id="@+id/nameInput"
            android:layout_width="314dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintStart_toEndOf="@+id/nameSpaceTitle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.362"
            app:layout_constraintVertical_bias="0.25"/>
    <TextView
            android:id="@+id/addressSpaceTitle"
            android:text="주소"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.158"
            app:layout_constraintVertical_bias="0.506"/>
    <EditText
            android:id="@+id/addressInput"
            android:layout_width="314dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintStart_toEndOf="@+id/addressSpaceTitle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.362"
            app:layout_constraintVertical_bias="0.508"/>
    <Button
            android:id="@+id/insertMemberBtn"
            android:text="회원 등록"
            android:textSize="20sp"
            android:layout_width="240dp"
            android:layout_height="55dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintVertical_bias="0.732"/>
    <EditText
            android:id="@+id/ageInput"
            android:layout_width="314dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintStart_toEndOf="@+id/ageSpaceTitle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.362"
            app:layout_constraintVertical_bias="0.375"/>
</androidx.constraintlayout.widget.ConstraintLayout>